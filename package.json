{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fab/nextjs": "0.4.0",
    "babel-plugin-module-resolver": "3.2.0",
    "babel-plugin-styled-components": "1.10.6",
    "babel-plugin-transform-flow-strip-types": "6.22.0",
    "compression": "1.7.4",
    "cookie": "0.4.0",
    "express": "4.17.1",
    "file-loader": "4.2.0",
    "jsesc": "2.5.2",
    "next": "9.1.1",
    "nprogress": "0.2.0",
    "query-string": "6.8.3",
    "raf-throttle": "2.0.3",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-keyframes": "0.2.3",
    "react-transition-group": "4.3.0",
    "retire": "2.0.3",
    "styled-components": "4.4.0",
    "styled-transition-group": "2.0.0"
  },
  "devDependencies": {
    "@percy/agent": "0.19.1",
    "@percy/puppeteer": "1.0.8",
    "babel-eslint": "10.0.3",
    "eslint": "6.5.1",
    "eslint-import-resolver-babel-module": "5.1.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-flowtype": "4.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-react": "7.16.0",
    "js-yaml": "3.13.1",
    "mocha": "6.2.1",
    "node-fetch": "2.6.0",
    "puppeteer": "1.20.0",
    "webpack-bundle-analyzer": "3.5.2"
  },
  "scripts": {
    "lint": "eslint . --color",
    "lint:fix": "eslint . --color --fix",
    "build": "next build",
    "build:fab": "FAB_BUILD=true npm run build && npm run fab:compile",
    "prefab:compile": "rm .next/static/assets/assets/fonts/* && echo 'NOTE: Clearing fonts so they dont get auto-served by @fab/compile'",
    "fab:compile": "FAB_BUILD=true fab-nextjs build --server=fab-server.js",
    "export": "next export",
    "start": "node server.js",
    "bundle-analyze": "NODE_ENV=production ANALYZE=1 next build",
    "security": "yarn audit && yarn run retire",
    "retire": "npx retire",
    "integration": "mocha --no-timeouts integration-tests/index.test.js",
    "percy": "percy exec -- mocha --no-timeouts integration-tests/percy.test.js",
    "integration:production": "TEST_HOST=https://buildkite.com/ yarn run integration"
  },
  "resolutions": {
    "**/js-yaml": ">=3.13.0"
  }
}
